{
  "apps" : [{
    "name": "sktelink",
    "script": "app.js",
    "ignore_watch" : ["node_modules", "logs"],
    "watch_options": {
      "followSymlinks": false
    },
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "error_file": "./logs/mocam.server.prod.stderr.log",
    "out_file": "./logs/mocam.server.prod.stdout.log",
    "pid_file": "./logs/mocam.server.prod.pid"
  }]
}

